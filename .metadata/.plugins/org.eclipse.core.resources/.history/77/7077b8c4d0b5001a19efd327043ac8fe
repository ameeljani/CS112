package Project;

public class Point {
	
	private int x;
	
	private int y;
	
	boolean pawndiagonal;
	
	public Point (int num1, int num2, boolean bool)
	{
		x=num1;
		y=num2;
		pawndiagonal=bool;
	}
	public int getx()
	{
		return x;
	}
	public int gety()
	{
		return y;
	}
	public boolean isDiagonal()
	{
		return pawndiagonal;
	}
	public boolean matches(int num1, int num2)
	{
		if(num1==x && num2==y)
		{
			return true;
		}
		else
		{
			return false;
		}
	}
	
	
	

}

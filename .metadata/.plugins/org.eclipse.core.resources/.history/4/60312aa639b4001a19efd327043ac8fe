package Project;

public class Piece {
	
	private String type;
	
	private String side;
	
	private int value;
	
	private Point location;
	
	public Piece(String type2,String side2, Point location2)
	{
		type=type2;
		side=side2;
		location=location2;
		
		
	}
	
	public String getType()
	{
		return type;
	}
	public String getside()
	{
		return side;
	}
	public int getValue()
	{
		return value;
	}
	public Point getLocation()
	{
		return location;
	}
	

}

package Project;

public class Piece {
	
	private String type;
	
	private String side;
	
	private int value;
	
	private String location;
	
	public String getType()
	{
		return type;
	}
	public String getside()
	{
		return side;
	}
	public int getValue()
	{
		return value;
	}
	public String getLocation()
	{
		return location;
	}
	

}

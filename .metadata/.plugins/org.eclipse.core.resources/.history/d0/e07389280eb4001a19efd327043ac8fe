package Project;

public class Piece {
	
	private String type;
	
	private String side;
	
	private int value;
	
	private int locationx;
	
	private int locationy;
	
	public String getType()
	{
		return type;
	}
	public String getside()
	{
		return side;
	}
	public int getValue()
	{
		return value;
	}
	public int getLocationx()
	{
		return locationx;
	}
	public int getLocationy()
	{
		return locationy;
	}
	

}
